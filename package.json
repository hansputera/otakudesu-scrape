    "phin": "^3.6.1"
  },
  "homepage": "https://github.com/hansputera/otakudesu-scrape",
  "devDependencies": {
    "@types/node": "16.11.39",
    "@typescript-eslint/eslint-plugin": "5.30.6",
    "@typescript-eslint/parser": "5.30.6",
    "eslint": "8.17.0",
    "eslint-config-google": "0.14.0",
    "rimraf": "3.0.2",
    "typedoc": "0.22.17",
    "typescript": "4.7.4"
  },
  "engines": {
    "node": ">=16.10.0"
  },
  "packageManager": "yarn@3.2.2"
